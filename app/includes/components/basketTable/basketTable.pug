mixin basketTable([...arr]) 
    table.basket-table 
        tr 
            th 
            th Наименование
            th Производитель
            th Цена
            th Количество
            th Сумма
            th.basket-table__remove 
        each basket in arr 
            tr 
                td.basket-table__img 
                    a(href="#") 
                        picture 
                            source(srcset=basket.webp, type="image/webp")
                            img(src=basket.img, alt="")
                td.basket-table__name 
                    p
                        a(href="#").product-card__name=basket.name 
                    .product-card__articule=basket.articule 
                        
                td=basket.developer
                td 
                    b=basket.price
                td 
                    if(basket.counter)
                        include ../../UI/counter/counter 
                    else 
                        span=basket.value
                td 
                    b=basket.total
                td.basket-table__remove 
                    button 
